@article{cook_SitesMilesianTerritory_1961,
	title = {Some sites of the {Milesian} territory},
	volume = {56},
	issn = {0068-2454, 2045-2403},
	url = {https://www.cambridge.org/core/product/identifier/S0068245400013496/type/journal_article},
	doi = {10.1017/S0068245400013496},
	abstract = {The territorial possessions of Miletus were acquired piecemeal over a number of centuries. They amounted to six or seven distinct pieces of land on the Asiatic mainland (apart from the adjacent islands, which again seem to have presented differences of status). A triple division of the Milesian territory on the mainland is already made by Herodotus. In the first place, in i. 18 he records that the Milesians suffered defeats at the hands of Sadyattes én te Limenēíōi chṓrēs tē̂s sphetérēs kaì en Maiándrou pedíōi It might at first sight appear that the Maeander plain here referred to was not Milesian territory. But Strabo (xiv. 647) seems to indicate that Magnesia, which lay far up the plain, suffered Milesian occupation after its destruction by the Treres in the seventh century; and there are various testimonies to Milesian possession of the lower Maeander plain in later times. Again, in vi. 20, Herodotus tells us that after the fall of Miletus (
              c
              . 494 B.C.) the captive citizens were removed by the Persians:
              
              
              The distinction between the part round the city and that in the plain evidently corresponds to that already observed in i. 18; and it is matched by the distinction drawn in an inscription of Miletus (of the first half of the second century B.C.) between the ‘Milesia’ proper and the Milēsíōn chṓra lying across the Latmic Gulf. Herodotus' triple division is thus clear: the ‘home territory’ around Miletus itself, the Milesian land in the plain, and third the mountain land, which is evidently Grion.},
	language = {en},
	urldate = {2021-12-13},
	journal = {The Annual of the British School at Athens},
	author = {Cook, John Manuel},
	month = nov,
	year = {1961},
	keywords = {13 Umlandforschung},
	pages = {90--101},
}

