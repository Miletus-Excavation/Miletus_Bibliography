@article{bruckner_CoastalChangesWestern_1997,
 abstract = {The Mediterranean coast of western Turkey witnessed one of the most extensive seaward shifts in the shoreline recorded in history. Famous ancient harbour cities like Ephesos, Miletos, Kaunas and also Troy are now several kilometres inland. High sediment loads of the rivers - often anthropogenetically induced - led to the silting up of the former sea ports; thus the cities lost one of their major functions. The phases of coastal progradation are deciphered by the interpretation of historical sources and geological evidence. A study of sediments and fauna indicates the milieu of deposition (marine, lagoonal, swampy, terrestrial) while radiocarbon dated material and artifacts provide the chronostratigraphical framework. Geoarchaeological research aims to reconstruct the former landscape and trace the phases of sedimentation in historic and prehistoric times. This paper presents information on the evolution of the deltas of B{\"u}y{\"u}k Menderes (Miletos ), K{\"u}{\c c}{\"u}k Menderes (Ephesos), Dalyan (Kaunas) and Karamenderes (Troy).},
 author = {Br{\"u}ckner, Helmut},
 journal = {Bulletin de l\textasciiacute Institut Oc{\'e}anographique Monaco, Beiheft},
 keywords = {11 Geographie und Geophysik, M{\"a}ander, Verlandung},
 pages = {63--74},
 title = {{C}oastal {C}hanges in {W}estern {T}urkey: {R}apid {D}elta {P}rogradation in {H}istorical {T}imes},
 volume = {18},
 year = {1997}
}

