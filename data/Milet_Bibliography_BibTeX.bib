@incollection{taschner_MiletKonservierungTourismus_2014,
 abstract = {Die Ausgrabungen in Milet haben eine lange Tradition, die bis ins 19. Jahrhundert zur{\"u}ckreicht. Von Anfang an haben die Ausgrabungen in Milet dazu beigetragen, grundlegende wissenschaftliche und methodische Fragen in der Arch{\"a}ologie aufzuwerfen; in Bezug auf Fragen der Erhaltung des kulturellen Erbes steht Milet auch heute noch vor gro{\ss}en Herausforderungen. Neben der gro{\ss}en Ausdehnung der Ausgrabungsst{\"a}tte selbst stellen regelm{\"a}{\ss}ige {\"U}berschwemmungen eine gro{\ss}e Bedrohung f{\"u}r den Schutz der Ruinen und die touristische Erschlie{\ss}ung der St{\"a}tte dar. Um weitere Sch{\"a}den zu verhindern, hat das Deutsche Arch{\"a}ologische Institut im Herbst 2012 eine Kampagne gestartet. In einem ersten Schritt wurden Symptome, Ursachen und Auswirkungen der {\"U}berschwemmungen analysiert. Auch wenn die Ergebnisse noch keine endg{\"u}ltigen Antworten liefern k{\"o}nnen, erm{\"o}glichen sie eine systematische Analyse, die bei der Entwicklung von Pr{\"a}ventionsma{\ss}nahmen f{\"u}r arch{\"a}ologische St{\"a}tten mit {\"a}hnlichen Problemen hilfreich sein kann.
Milet wird jedes Jahr im Sp{\"a}therbst {\"u}berflutet, wobei einige Ruinen bis zum Sommer des folgenden Jahres {\"u}berschwemmt sind. In diesen Monaten ist das Zentrum der St{\"a}tte nicht zug{\"a}nglich, und kleinere Geb{\"a}udeteile sind nicht sichtbar. Ein weiteres Problem stellen Mikroorganismen dar, die sich in stehendem Wasser bei steigenden Temperaturen ansiedeln und auf Marmor und anderen Steinen feste Krusten bilden, die diese Oberfl{\"a}chen dauerhaft {\"u}berziehen. Neben diesen potenziellen Schadensfaktoren werden in diesem Beitrag verschiedene andere negative Auswirkungen der {\"U}berschwemmungen er{\"o}rtert. Insbesondere werden L{\"o}sungen vorschlagen, die sich aus den ver{\"a}nderten Rahmenbedingungen vor Ort ergeben.},
 address = {Istanbul},
 author = {Taschner, Moritz},
 booktitle = {Heritage in context : Konservierung und Site Management im nat{\"u}rlichen, urbanen und sozialen Raum. Do{\u g}al, kentsel ve sosyal {\c c}er{\c c}evede koruma ve alan y{\"o}netimi},
