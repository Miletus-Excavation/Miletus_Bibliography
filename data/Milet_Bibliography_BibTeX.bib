@article{emme_KeinKaiserkultRekonstruktion_2014,
	title = {Kein {Kaiserkult}. {Rekonstruktion} und {Deutung} der ‚ara augusti’ im {Buleuterion} in {Milet}},
	volume = {64},
	abstract = {Das Monument im Hof des Buleuterions von Milet gilt seit den Untersuchungen von K. Tuchelt als prominentes Beispiel eines augusteischen Kaiserkultaltars. Demgegenüber vermag eine eingehende Analyse der in der Grabungspublikation vorgelegten Befunde aufzuzeigen, dass der Bau aller Wahrscheinlichkeit nach nicht als Altar zu rekonstruieren ist. Auch auf epigraphischer Grundlage ist eine Verbindung mit dem Kaiserkult nicht zu belegen. Im vorliegenden Beitrag wird stattdessen eine Rekonstruktion des Monuments als späthellenistische Memorialarchitektur vorgestellt. Damit wird die erstmals von H. Knackfuß vorgeschlagene Deutung als Memorialbau (»Ehrengrab«) über einhundert Jahre nach Vorlage der Grabungspublikation ausgehend vom archäologischen Befund untermauert.},
