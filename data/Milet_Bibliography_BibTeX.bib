 language = {en},
 number = {1},
 pages = {67--156},
 title = {{A}n {A}ncient {C}ave {S}anctuary underneath the {T}heatre of {M}iletus, {B}eauty, {M}utilation, and {B}urial of {A}ncient {S}culpture in {L}ate {A}ntiquity, and the {H}istory of the {S}eaward {D}efences},
