@article{reich_UntersuchungHafenbeckenMilets_2021,
	title = {Untersuchung der {Hafenbecken} {Milets} unter nautischen {Gesichtspunkten}},
	volume = {21},
	abstract = {The ancient city of Miletus (Turkey) had, economically seen, an advantageous position as it was located at the exit of the Meander valley on the South Ionian coast. According to Strabo (geogr. 14,1,6) the city had four harbours. Based on the archaeological evidence and the geoarchaeological investigations in and around Miletus, four potential harbours can be identified to match Strabon’s statement: Lion Harbour, Theatre Harbour, Humeitepe Harbour, and East Harbour. The four harbours were investigated with a focus on their nautical properties for the period from the 5th century BC to the 3rd century AD. A holistic approach was chosen for the reconstruction of the harbours. The following factors were taken into account: archaeological evidence, topographical and artificial landmarks, geoarchaeological and geophysical analyses, written sources, shipwrecks, wind conditions, currents, and sea conditions. Wind and waves could hinder the approach to the harbours at certain times. These and other possible hindrances could potentially be avoided by the orientation on landmarks in and around the city. Overall, Miletus can be considered a safe port, with a large trade volume and potential.},
	journal = {Skyllis. Zeitschrift für maritime und limnische Archäologie und Kulturgeschichte},
	author = {Reich, Alexander},
	year = {2021},
	keywords = {07 Architektur, 07-08 Architektur: Häfen und maritime Infrastruktur},
	pages = {91--99},
}

