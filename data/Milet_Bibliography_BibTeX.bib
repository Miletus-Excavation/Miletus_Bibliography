@article{vongraeve_ArbeitenMiletJahren_2019,
  title = {Die Arbeiten in Milet in den Jahren 2006\textendash 2011},
  author = {{von Graeve}, Volkmar},
  year = {2019},
  journal = {Arch\"aologischer Anzeiger},
  volume = {2019},
  number = {1},
  pages = {118--227},
  doi = {10.34780/S9GK-V324},
  abstract = {Den Schwerpunkt der Arbeiten bildeten die Ausgrabungen im milesischen Aphroditeheiligtum \guilsinglright von Oikus\guilsinglleft, das im Rahmen des archaischen Programms entdeckt wurde und dessen Freilegung nun abgeschlossen ist. Das Zentrum seiner Topographie bildet ein Gipfelplateau als Standort der sukzessiv erfolgten Tempelbauten, von denen nur die Existenz eines kleinen Baus aus dem fr\"uhen 6. Jh. v. Chr. und eines gro\ss en sp\"atarchaischen Tempels gesichert ist. Weitere topographische Fixpunkte sind die Westterrasse, deren Anf\"ange in einem aufgef\"ullten Steinbruch liegen und die mit den Funden aus dieser Auff\"ullung einen gro\ss en Teil der Votivgaben des Heiligtums geliefert hat, au\ss erdem der Ostabhang mit einem Bothros, der in der Zeit um 630 v. Chr. geschlossen wurde und der mit dieser Datierung eine chronologische Neuorientierung im 7. Jh. v. Chr. notwendig macht. Gegraben wurde au\ss erdem an einer Stra\ss e n\"ordlich des Kalabaktepe, die nach der geophysikalischen Prospektion zum Stra\ss enraster des sp\"atarchaischen Stadtplans von Milet geh\"ort hat. Die Grabung hat gezeigt, dass hier infolge des Niedergangs der Stadt nach der Perserzerst\"orung das neue urbanistische Konzept nicht vollst\"andig umgesetzt wurde.},
  keywords = {01 Grabungs und Arbeitsberichte,02 Allgemeine Darstellungen / Topographie,02-04 Topographie: archaische Zeit,07 Architektur,07-01 Architektur: Varia,08 Kulte und Kulteinrichtungen,08-06 Kulte: Aphrodite von Oikous,08-07 Kulte: Artemis Kithone,14 Forschungsgeschichte,15 Ruinen und Geländepflege; Restaurierung,Aphroditeheiligtum,Archaisch,Faustinathermen,Hafen,Hafentor,Heiligtum,Nekropole,Perserkriege,Restaurierung,Römisch,Stadtplan,Straßen,Straßenraster,Tor,Wohnbebauung},
  timestamp = {2022-12-20T10:37:21Z}
}

