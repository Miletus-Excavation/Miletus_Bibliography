@article{steinmann_FindsMiletusXXXII_2020,
  title = {Finds from {{Miletus XXXII}}. {{Clay Rings}} from the {{Sanctuary}} of {{Dionysos}} in {{Miletus}}},
  author = {Steinmann, Lisa},
  year = {2020},
  journal = {Arch\"aologischer Anzeiger},
