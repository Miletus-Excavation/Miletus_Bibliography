 author = {Zimmermann, Konrad},
 doi = {10.34780/s767-fk54},
 journal = {Arch{\"a}ologischer Anzeiger},
 keywords = {03 Funde aus Milet, 03-06 Funde: Terrakotta, 08 Kulte und Kulteinrichtungen, 08-06 Kulte: Aphrodite von Oikous, Archaisch, Architektur, Architekturterrakotten, Lydien, Terrakotten, Weihgeschenk, Zeytintepe},
