@book{niewohner_ByzantinischenBasilikenMilet_2016,
 abstract = {Der Band legt Ergebnisse von {\"u}ber hundert Jahren Grabung vor. Zwei Emporenbasiliken wurden in den 1900er und 1970er Jahren ausgegraben, eine Transeptbasilika in einem christlichen Friedhofsbezirk mit Heiligengrab in den 2000er Jahren. Eine vierte Kirche l{\"a}{\ss}t sich anhand von Bauskulptur rekonstruieren. S{\"a}mtliche Bauten stammen aus dem 6./7. Jh. n. Chr. und wurden von lokalen Werkst{\"a}tten ausgestattet. Das Formenrepertoire ist konservativ und voller R{\"u}ckbez{\"u}ge auf das antike Erbe. Die Selbstbezogenheit ist typisch f{\"u}r die alten Gro{\ss}st{\"a}dte Westkleinasiens und kann erkl{\"a}ren, warum es dort nicht zur Auspr{\"a}gung eines einheitlichen neuen Baustils kam.},
