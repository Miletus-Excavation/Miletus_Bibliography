  doi = {10.34780/a847-7wqa},
  abstract = {Der Bischofspalast im Stadtzentrum von Milet wurde im fr\"uhen 20. Jh. von Theodor Wiegand entdeckt und in den 1970er Jahren von Wolfgang M\"uller-Wiener ausgegraben, aber bislang nicht publiziert. Dies wird hiermit nachgeholt, nachdem 2013 weitere Ausgrabungen und eine Neuuntersuchung des Komplexes vorgenommen werden konnten. Der Bischofspalast wurde in der 1. H\"alfte des 5. Jhs. n. Chr. an der Stelle eines sp\"atr\"omischen Peristylhauses aus dem 3. Jh. n. Chr. errichtet, das im 4. Jh. n. Chr. umgebaut worden war. \"Uber dem zerst\"orten Palast war noch eine T\"opferei aus der Zeit des t\"urkischen Emirats von Mente\c{s}e (14./15. Jh.) zu verzeichnen sowie noch sp\"ater und h\"oher osmanische Bebauung. Der Bischofspalast war eng mit der ihm s\"udlich benachbarten Michaelskirche verbunden, die offenbar als bisch\"ofliches Oratorium und Palastkapelle diente. Die Kapelle, ein Vestib\"ul in Form eines langgestreckten Apsidensaals, ein verh\"altnism\"a\ss ig gro\ss er Hauptsaal sowie der Verzicht auf einen zentralen Peristylhof unterscheiden den Bischofspalast einerseits von sp\"atantiken Peristylh\"ausern, verbinden ihn andererseits mit dem sog. Byzantinischen Palast von Ephesos und gehen wohl auf die ge\"anderten Repr\"asentationsbed\"urfnisse fr\"uhbyzantinischer Amtstr\"ager zur\"uck.},
  keywords = {01 Grabungs und Arbeitsberichte,07 Architektur,07-01 Architektur: Varia,07-09 Architektur: Wohngebäude,Bischofspalast,Byzantinisch,Menteşe,Mosaik,Peristylhaus,Römisch,Spätantik,Wohnbebauung},
  timestamp = {2022-12-20T10:36:37Z}
}

@incollection{niewohner_ByzantinePreservationAncient_2019,
  title = {Byzantine Preservation of Ancient Monuments at Miletus in Caria. Christian Antiquarianism in Western Asia Minor},
  booktitle = {Die Weltchronik des Johannes Malalas im Kontext sp\"atantiker Memorialkultur},
  author = {Niew{\"o}hner, Philipp},
  editor = {Borsch, Jonas and Gengler, Olivier and Meier, Mischa},
  year = {2019},
  series = {Malalas-Studien},
  number = {3},
  pages = {191--216},
  publisher = {Franz Steiner Verlag},
  address = {Stuttgart},
  abstract = {This paper tries to understand the extraordinary preservation of ancient Miletus throughout the early Byzantine period, which did not occur due to abandonment or neglect but due to intentional conservation. This qualifies as Byzantine antiquarianism, appears to have been unrelated to paganism, and was practised by Christians. Comparison with similarly well-preserved ancient cityscapes at Aphrodisias and Ephesus suggests that Byzantine antiquarianism was inspired by the ancient monuments themselves, because they continued to distinguish old cities even after their former privileges had been abolished by Byzantine administration. Other Byzantine settlements elsewhere in Anatolia lacked ancient monuments and displayed no antiquarian tendencies. Conversely, a focus on the preservation of ancient cityscapes may also explain why western Asia Minor, despite being the most urbanised part of the region, did not develop a Byzantine architectural style of its own.},
  keywords = {02 Allgemeine Darstellungen / Topographie,02-08 Topographie: Spät und Nachantike,07 Architektur,07-01 Architektur: Varia,15 Ruinen und Geländepflege; Restaurierung,Byzantinisch},
  timestamp = {2022-12-15T17:33:19Z}
}

@article{niewohner_ByzantineSettlementHistory_2016,
  title = {The Byzantine Settlement History of Miletus and Its Hinterland \textendash{} Quantitative Aspects. Stratigraphy, Pottery, Anthropology, Coins, and Palynology},
  author = {Niew{\"o}hner, Philipp and Demirel, Arzu and Izdebski, Adam and Sancaktar, Hacer and Schwerdt, Nico and St{\"u}mpel, Harald},
  year = {2016},
  journal = {Arch\"aologischer Anzeiger},
  volume = {2016},
  number = {2},
  pages = {225--290},
  doi = {10.34780/DYAP-714E},
  urldate = {2022-02-09},
  abstract = {The Byzantine settlement history of Miletus, an ancient harbour city on the west coast of Asia Minor, was long obscured by a lack of quantitative data. This paper provides such data and fills in some crucial gaps in our knowledge of the urban development and in our understanding of the rural hinterland. The city appears to have lost much of its former population by the 6th century A.D., which was a particularly prosperous time for the surrounding villages. Thereafter, agriculture continued relatively undisturbed throughout the Persian and Arab invasions, the middle Byzantine period \textendash{} with considerable prosperity at rural sites \textendash{} and the Turkish conquest. The city, in contrast, was deserted during the middle Byzantine period. Middle Byzantine prosperity turns out to have been a rural phenomenon of the agricultural hinterland, and urbanism was dead long before the arrival of the Turks, who had to rebuild Miletus under the new name of Balat.},
  keywords = {02 Allgemeine Darstellungen / Topographie,02-08 Topographie: Spät und Nachantike,03 Funde aus Milet,03-03 Funde: Bauteile; Bauornamentik,03-05 Funde: Keramik,03-05-08 Keramik: Spät und Nachantike,03-09 Funde: Münzen,05 Archäozoologie; Archäobotanik; Anthropologie,11 Geographie und Geophysik,13 Umlandforschung,Byzantinisch,Chora,Keramik,Menteşe,Spätantik},
  timestamp = {2023-01-26T12:12:01Z}
}

