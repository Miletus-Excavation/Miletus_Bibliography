@article{slawisch_ProcessionsPropagandaPixels_2018,
 abstract = {The Sacred Way connecting the city of Miletos to the sanctuary of Didyma has long been considered one of the best-documented examples of a processional road from the ancient world. Views of the road have become ossified around an orthodox reconstruction of the route, which is assumed to have remained relatively static from the Archaic to the Roman period. A reexamination of the full epigraphic and archaeological evidence, incorporating the latest research in the region, highlights the many gaps in our knowledge and the possibility that the route and identity of the Sacred Way may have changed substantially through time. Computational modeling of the local topography confirms the feasibility of alternative routes and the effect that probable long-term landscape change around Panormos might have had. This article calls for a fresh characterization of the Sacred Way from Miletos to Didyma, which envisages multiple periods of (re)invention and (re)construction from the Archaic period right up to the modern day.},
 author = {Slawisch, Anja and Wilkinson, Toby Christopher},
 doi = {10.3764/aja.122.1.0101},
 issn = {0002-9114, 1939-828X},
 journal = {American Journal of Archaeology},
 keywords = {08 Kulte und Kulteinrichtungen, 08-03 Kulte: Heilige Stra{\ss}e (Milet\textbackslash textless\textbackslash textgreaterDidyma), Didyma, Heilige Stra{\ss}e, Landschaft, Prozession},
 language = {en},
 number = {1},
 pages = {101--143},
 title = {{P}rocessions, {P}ropaganda, and {P}ixels: {R}econstructing the {S}acred {W}ay {B}etween {M}iletos and {D}idyma},
 url = {https://www.journals.uchicago.edu/doi/10.3764/aja.122.1.0101},
 urldate = {2022-02-11},
 volume = {122},
 year = {2018}
}

@incollection{bresson_PidasaMiletosKarian_2019,
 abstract = {Herodotus (6.20) tells us how after the revolt and defeat of Miletos of 494 BC its mountain territory was given to the Karians of Pedasa. Modern literature has hesitated on the identification of this Pedasa, although more often it has been considered that the city of that name was the homonymous Pedasa located north of Halikarnassos. However, this identification should be abandoned. Despite the apparent name difference, it can be proved that the city in question was Pidasa of Mount Grion, the eastern neighbor of Miletos. Obviously this city did not flourish as it might have. But this does not mean that it was a poor and negligible state, doomed to quickly disappear. In fact, the longue dur{\'e}e story of Pidasa is better explained by the fact that its most direct access to the sea was obstructed by the very presence of Miletos on the coast.},
 address = {Istanbul},
 author = {Bresson, Alain},
 booktitle = {Karia Arkhaia: La Carie, des origines {\`a} la p{\'e}riode pr{\'e}iode pr{\'e}-h{\'e}katomnide, Istanbul, 14-16 novembre 2013},
 editor = {Henry, Olivier and Konuk, Koray},
 keywords = {10 politische Geschichte, 13 Umlandforschung, Karien},
 pages = {227--246},
 publisher = {Zero Books},
 title = {{P}idasa, {M}iletos, and the {K}arian {R}evolt},
 year = {2019}
}

