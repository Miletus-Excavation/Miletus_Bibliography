
@article{akcer-on_RedatingFormationLake_2020,
	title = {Redating the formation of {Lake} {Bafa}, western {Turkey}: {Integrative} geoarchaeological methods and new environmental and dating evidence},
	volume = {35},
	issn = {0883-6353, 1520-6548},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/gea.21791},
	doi = {10.1002/gea.21791},
	abstract = {The ancient Gulf of Latmos is an iconic example of a dynamic landscape and humankind's historical relationship with it. Using extensive new primary data and original models for calibrating radiocarbon dates in transitional lagoon environments, we demonstrate that Lake Bafa (or Bafa Gölü, in Turkish) formed at a much earlier date than previously thought. In questioning the logical process by which previous dates were achieved, we reexamine the relationship between sedimentological data, archaeology and written history. We reassert the need to establish independently dated environmental data sets as the foundation of regional studies as distinct from archaeological and historical interpretive processes. We conclude that Lake Bafa slowly transitioned to become an isolated lagoon sometime between the end of the second millennium B.C. and end of the first millennium B.C.; becoming a fully closed brackish lake during the second millennium A.D. This marks a major shift in our understanding of the nature of human occupation and activity here during the last four millennia but also in the way we date ancient lagoons and integrate historical and environmental data in general.},
	language = {en},
	number = {5},
	urldate = {2022-02-15},
	journal = {Geoarchaeology},
	author = {Akçer-Ön, Sena and Greaves, Alan M. and Manning, Sturt W. and Ön, Zeki Bora and Çağatay, M. Namık and Sakınç, Mehmet and Oflaz, Aslı and Tunoğlu, Cemal and Salihoğlu, Rana},
	year = {2020},
	keywords = {11 Geographie und Geophysik, Bafa Gölü, Geoarchäologie, Geologie, Mäander, Radiokarbondatierung},
	pages = {1--19},
}

