  author = {Bresson, Alain},
  editor = {Henry, Olivier and Konuk, Koray},
  year = {2019},
  pages = {227--246},
  publisher = {{Zero Books}},
  address = {{Istanbul}},
  abstract = {Herodotus (6.20) tells us how after the revolt and defeat of Miletos of 494 BC its mountain territory was given to the Karians of Pedasa. Modern literature has hesitated on the identification of this Pedasa, although more often it has been considered that the city of that name was the homonymous Pedasa located north of Halikarnassos. However, this identification should be abandoned. Despite the apparent name difference, it can be proved that the city in question was Pidasa of Mount Grion, the eastern neighbor of Miletos. Obviously this city did not flourish as it might have. But this does not mean that it was a poor and negligible state, doomed to quickly disappear. In fact, the longue dur\'ee story of Pidasa is better explained by the fact that its most direct access to the sea was obstructed by the very presence of Miletos on the coast.},
  keywords = {10 politische Geschichte,13 Umlandforschung,Karien}
}

