  title = {Chronology for the {{Aegean Late Bronze Age}} 1700-1400 {{B}}.{{C}}.},
  author = {Manning, Sturt W. and Ramsey, Christopher Bronk and Kutschera, Walter and Higham, Thomas and Kromer, Bernd and Steier, Peter and Wild, Eva M.},
