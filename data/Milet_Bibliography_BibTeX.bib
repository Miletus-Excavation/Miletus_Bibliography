@book{schaus_FundeAusMilet_2020,
  title = {Funde Aus {{Milet}} 4. {{Laconian}} and {{Chian}} Fine Ware Pottery at {{Miletus}}},
  author = {Schaus, Gerald P.},
  year = {2020},
  series = {Milet. {{Ergebnisse}} Der {{Ausgrabungen}} Und {{Untersuchungen}} Seit Dem {{Jahr}} 1899},
  number = {5,4},
  publisher = {{Harrassowitz}},
  address = {{Wiesbaden}},
  abstract = {Im vorliegenden Band wird einer der umfangreichsten Best\"ande an archaisch-lakonischer Keramik, der bisher jenseits des Mittelmeeres entdeckt wurde, untersucht und der - gemessen an der Quantit\"at - den Funden aus Samos gleichkommt und diese in Teilen sogar \"ubertrifft. Individuelle Malerh\"ande und Werkst\"atten werden im Rahmen der Untersuchung bestimmt und unterschieden. In \"ahnlicher Weise geh\"ort auch die chiotische Feinkeramik aus Milet zu den zweit- oder drittgr\"o\ss ten Fundkomplexen in Zielgebieten des \"uberseeischen Exports. Die Funde aus Milet tragen zur Kenntnis der wichtigen lakonischen und chiotischen Gef\"a\ss werkst\"atten der archaischen Zeit sowie zur Kenntnis des Gebrauchs von Keramik und deren Verteilungsmuster \"uber das Mittelmeer hinweg bei.},
  isbn = {978-3-447-11428-8},
  langid = {english},
  keywords = {03 Funde aus Milet,03-05 Funde: Keramik,03-05-04 Keramik: archaische Zeit,Zeytintepe}
}

@incollection{schaus_LaconiaEastGreece_2018,
  title = {Laconia and {{East Greece}}. {{Cultural Exchange}} in the {{Archaic Period}}},
  booktitle = {Archaic and {{Classical Western Anatolia}} : New {{Perspectives}} in {{Ceramic Studies}}: {{In}} Memoriam {{Prof}}. {{Crawford H}}. {{Greenewalt Jr}}: Proceedings of the {{Second KERAMOS International Conference}} at {{Ege University}}, {{Izmir}}, 3-5 {{June}}, 2015},
  author = {Schaus, Gerald P.},
  editor = {G{\"u}l G{\"u}rtekin Demir, Rafet and Cevizo{\u g}lu, H{\"u}seyin and Polat, Yasemin and Polat, G{\"u}rcan},
  year = {2018},
  series = {Colloquia {{Antiqua}}},
  number = {19},
  pages = {285--302},
  publisher = {{Peeters}},
  address = {{Leuven}},
  abstract = {The notion of a `special relationship' between Sparta and Samos in the Archaic period has been widely accepted since it was first proposed by E.A. Lane in 1934, and carefully discussed by P. Cartledge in 1982, based especially on Laconian pottery finds at Samos, and information in Herodotus. It has become clear in more recent years, however, that Laconians were in regular contact with East Greeks generally, as well as with Lydians, in the Archaic period, for political, economic and cultural reasons. Recent Laconian pottery finds at Miletus suggest that Samos was not an exclusive destination in East Greece for significant quantities of this ware.},
  keywords = {03 Funde aus Milet,03-05 Funde: Keramik,03-05-04 Keramik: archaische Zeit,Archaisch}
}

@article{schaus_PainterTarantoFish_2015,
  title = {The {{Painter}} of the {{Taranto Fish}}. {{An Early Laconian Black-Figure Vase Painter}}},
  author = {Schaus, Gerald P.},
  year = {2015},
  journal = {Jahrbuch des Deutschen Arch\"aologischen Instituts},
  volume = {130},
  pages = {1--73},
  abstract = {The Painter of the Taranto Fish is the earliest distinct personality in Laconian vase painting. Previously he has had only thirteen or fourteen vases attributed to his hand, but with the discovery of Laconian pottery at Miletus by this Painter, many other known vases, including some especially distinguished ones, can be attributed to him either directly or with strong probability, including the name vases of the Grammichele Painter and the Painter of Lakaina 1016. This changes our knowledge of his work significantly, placing him among the major Laconian painters, and an important influence in the development of the Laconian style ca. 595 \textendash{} 575 B.C. His work is found often in export markets, especially Italy-Sicily and the eastern Aegean, and from this it is clear that he helped to establish a solid customer base for Laconian pottery overseas.},
  keywords = {03 Funde aus Milet,03-05 Funde: Keramik,03-05-04 Keramik: archaische Zeit,Keramik - Gefäßmalerei,Schwarzfigurig}
}

