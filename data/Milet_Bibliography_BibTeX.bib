 pages = {595--606},
 title = {{W}er lie{\ss} in der {G}rabinschrift {I}.Milet {V}I 2,570 sieben {Z}eilen eradieren?},
 url = {https://www.degruyter.com/document/doi/10.1515/zrgr-2021-0017/html},
