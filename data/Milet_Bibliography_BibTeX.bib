@book{henke_ZyprischenTerrakottenAus_2017,
 abstract = {In vielen s{\"u}dostgriechischen Heiligt{\"u}mern stellen archaische zyprische Terrakotten eine reiche Fundgruppe dar. Nach den j{\"u}ngeren Grabungen l{\"a}sst sich auch Milet mit einer stattlichen Fundmenge diesen Orten anschlie{\ss}en. In dem Band werden die neuen milesischen Funde wissenschaftlich vorgelegt und ihre Stellung innerhalb des s{\"u}dost{\"a}g{\"a}ischen Gesamtkomplexes zyprischer Koroplastik diskutiert. Dabei steht erneut dessen stilchronologische und produktionstechnische Einordnung im Vordergrund. Aufbauend auf den regionalstilistischen Studien Sabine Fourriers auf Zypern werden die engeren werkstattspezifischen Verbindungen der s{\"u}dost{\"a}g{\"a}ischen Funde untersucht und es wird nach deren Herkunft auf Zypern gefragt. Hierbei gelingen u. a. erg{\"a}nzende Einblicke in die verschiedenen Herstellungsprozesse der Terrakotten sowie der Nachweis von unterschiedlichen Serien an hohl und massiv mit Matrizen hergestellten Figuren. In Einzelf{\"a}llen scheinen sogar individuelle Modellierungen konkreter Koroplasten oder eng zusammenarbeitender Handwerkergruppen erkennbar, die es erlauben, ein umfangreicheres {\OE}uvre {\"u}ber verschiedene Figurenformate und Produktionsweisen hinweg nachzuzeichnen. Damit verengt sich das Bild f{\"u}r den Gro{\ss}teil des s{\"u}dost{\"a}g{\"a}ischen Fundmaterials, f{\"u}r das schon fr{\"u}h eine auff{\"a}llige stilistische Homogenit{\"a}t erkannt wurde, auf sehr wenige in einem engeren zeitlichen wie r{\"a}umlichen Rahmen arbeitende Werkst{\"a}tten aus dem unmittelbaren topographischen Umfeld von Salamis auf Zypern.
Die Ergebnisse erweitern und sch{\"a}rfen die bisherigen stilistischen und produktionstechnischen Untersuchungen, k{\"o}nnen diese aber auch in Einzelf{\"a}llen korrigieren. Auf Grundlage der neuen Grabungsergebnisse in Milet und der {\"U}berpr{\"u}fung der samischen Kontexte zyprischer Koroplastik wird eine neue Datierung f{\"u}r den Import der Terrakotten in die S{\"u}dost{\"a}g{\"a}is vorgeschlagen, der kontr{\"a}r zu den bisherigen Ans{\"a}tzen innerhalb weniger Jahrzehnte bis ca. 630 v. Chr. stattgefunden hat. Daneben leiten die neuen Funde an wahrscheinlich in Milet hergestellten zyprisierenden Terrakotten zur Diskussion von technischen Anregungen auf das s{\"u}dost{\"a}g{\"a}ische Koroplastenhandwerk {\"u}ber, die die heimischen Handwerker durch Autopsie der umfangreichen zyprischen Importe erhalten haben k{\"o}nnten. Schlie{\ss}lich werden die Terrakotten als Zeugnisse der regen zwischenkulturellen Austauschprozesse im fr{\"u}heisenzeitlichen Mittelmeerraum hervorgehoben und nach der Identit{\"a}t der Stiftenden gefragt. Dabei k{\"o}nnten spezifische Verteilungsmuster der Figuren darauf hindeuten, dass sie nach konkreten technischen wie inhaltlichen Aspekten von der Lokalbev{\"o}lkerung ausgew{\"a}hlt wurden.},
 address = {Berlin},
 author = {Henke, Jan-Marc},
 keywords = {03 Funde aus Milet, 03-06 Funde: Terrakotta, 08 Kulte und Kulteinrichtungen, 08-06 Kulte: Aphrodite von Oikous, Aphroditeheiligtum, Archaisch, Terrakotten, Werkstatt, Zeytintepe, Zypern},
 number = {7},
 publisher = {Deutsches Arch{\"a}ologisches Institut},
 series = {Milesische Forschungen},
 title = {{D}ie zyprischen {T}errakotten aus {M}ilet: {N}eue {\"U}berlegungen zur {E}inordnung der archaischen zyprischen {T}errakotten aus ost{\"a}g{\"a}ischen {F}undkontexten und ihrer werkstattspezifischen {Z}uweisung},
 url = {https://www.doi.org/10.34780/1h67-m6dd},
 year = {2017}
