@incollection{taschner_MiletKonservierungTourismus_2014,
  title = {Milet \textendash{} Konservierung, Tourismus und Umweltbedingungen. Milet \textendash{} Koruma, Turizm ve \c{C}evre Ko\c{s}ullar\i},
  booktitle = {Heritage in context : Konservierung und Site Management im nat\"urlichen, urbanen und sozialen Raum. Do\u{g}al, kentsel ve sosyal \c{c}er\c{c}evede koruma ve alan y\"onetimi},
  author = {Taschner, Moritz},
  editor = {Bachmann, Martin and Maner, {\c C}i{\u g}dem and Tezer, Se{\c c}il and G{\"o}{\c c}men, Duygu},
  year = {2014},
  series = {Miras},
  number = {2},
  pages = {253--268},
  publisher = {Ege Yay\i nlar\i},
  address = {Istanbul},
  abstract = {Die Ausgrabungen in Milet haben eine lange Tradition, die bis ins 19. Jahrhundert zur\"uckreicht. Von Anfang an haben die Ausgrabungen in Milet dazu beigetragen, grundlegende wissenschaftliche und methodische Fragen in der Arch\"aologie aufzuwerfen; in Bezug auf Fragen der Erhaltung des kulturellen Erbes steht Milet auch heute noch vor gro\ss en Herausforderungen. Neben der gro\ss en Ausdehnung der Ausgrabungsst\"atte selbst stellen regelm\"a\ss ige \"Uberschwemmungen eine gro\ss e Bedrohung f\"ur den Schutz der Ruinen und die touristische Erschlie\ss ung der St\"atte dar. Um weitere Sch\"aden zu verhindern, hat das Deutsche Arch\"aologische Institut im Herbst 2012 eine Kampagne gestartet. In einem ersten Schritt wurden Symptome, Ursachen und Auswirkungen der \"Uberschwemmungen analysiert. Auch wenn die Ergebnisse noch keine endg\"ultigen Antworten liefern k\"onnen, erm\"oglichen sie eine systematische Analyse, die bei der Entwicklung von Pr\"aventionsma\ss nahmen f\"ur arch\"aologische St\"atten mit \"ahnlichen Problemen hilfreich sein kann. Milet wird jedes Jahr im Sp\"atherbst \"uberflutet, wobei einige Ruinen bis zum Sommer des folgenden Jahres \"uberschwemmt sind. In diesen Monaten ist das Zentrum der St\"atte nicht zug\"anglich, und kleinere Geb\"audeteile sind nicht sichtbar. Ein weiteres Problem stellen Mikroorganismen dar, die sich in stehendem Wasser bei steigenden Temperaturen ansiedeln und auf Marmor und anderen Steinen feste Krusten bilden, die diese Oberfl\"achen dauerhaft \"uberziehen. Neben diesen potenziellen Schadensfaktoren werden in diesem Beitrag verschiedene andere negative Auswirkungen der \"Uberschwemmungen er\"ortert. Insbesondere werden L\"osungen vorschlagen, die sich aus den ver\"anderten Rahmenbedingungen vor Ort ergeben.},
  keywords = {11 Geographie und Geophysik,15 Ruinen und Geländepflege; Restaurierung,Überschwemmung},
  timestamp = {2022-12-15T17:33:44Z}
}

