@phdthesis{cheba_BiogenInduzierteKalkkrusten_1999,
 abstract = {Die Marmorobjekte in der Ausgrabungsst{\"a}tte von Milet weisen an ihrer Oberfl{\"a}che stark verfremdende Krusten auf. Als Ursache f{\"u}r die Krustenbildung wird die zyklische {\"U}berflutung weiter Teile des Grabungsgel{\"a}ndes angesehen. In dieser Diplomarbeit wird die Mitwirkung von Mikroorganismen am Entstehungsprozess dieser Krusten untersucht. Neben der Charakterisierung unterschiedlicher Auspr{\"a}gungsformen der Krusten wird durch verschiedene Untersuchungen ihre Zusammensetzung und ihre Auswirkung auf die Marmorsubstanz bestimmt. Anhand der auftretenden Schadensph{\"a}nomene an den Marmorobjekten wird deutlich, dass die Krusten ein Schadenspotential darstellen. Die Entwicklung von geeigneten Ma{\ss}nahmen zu ihrer Abnahme bildet einen weiteren Schwerpunkt dieser Arbeit.},
 address = {K{\"o}ln},
