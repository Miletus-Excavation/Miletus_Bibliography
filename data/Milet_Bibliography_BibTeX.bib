@phdthesis{cheba_BiogenInduzierteKalkkrusten_1999,
  type = {{Diplomarbeit}},
  title = {{Biogen induzierte Kalkkrusten auf Marmorobjekten in der Ausgrabungsst\"atte Milet/T\"urkei. Untersuchung zum Krustenaufbau und Entwicklung von Konzepten zu ihrer Abnahme}},
  author = {Cheba, Sabina},
  year = {1999},
  address = {{K\"oln}},
  abstract = {Die Marmorobjekte in der Ausgrabungsst\"atte von Milet weisen an ihrer Oberfl\"ache stark verfremdende Krusten auf. Als Ursache f\"ur die Krustenbildung wird die zyklische \"Uberflutung weiter Teile des Grabungsgel\"andes angesehen. In dieser Diplomarbeit wird die Mitwirkung von Mikroorganismen am Entstehungsprozess dieser Krusten untersucht. Neben der Charakterisierung unterschiedlicher Auspr\"agungsformen der Krusten wird durch verschiedene Untersuchungen ihre Zusammensetzung und ihre Auswirkung auf die Marmorsubstanz bestimmt. Anhand der auftretenden Schadensph\"anomene an den Marmorobjekten wird deutlich, dass die Krusten ein Schadenspotential darstellen. Die Entwicklung von geeigneten Ma\ss nahmen zu ihrer Abnahme bildet einen weiteren Schwerpunkt dieser Arbeit.},
  langid = {ngerman},
  school = {FH K\"oln - Institut f\"ur Konservierungs- und Restaurierungswissenschaften},
  keywords = {13 Umlandforschung,15 Ruinen und Geländepflege; Restaurierung}
}

