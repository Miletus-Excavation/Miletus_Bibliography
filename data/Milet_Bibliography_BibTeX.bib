@article{dietrich_ExposedBedrockMiletus_2016,
	title = {Exposed bedrock in {Miletus} and {Priene}: an overlooked aspect of {Hellenistic} and {Imperial}‑era {Urbanism}},
	volume = {62/2},
	doi = {10.3917/arch.162.0303},
	abstract = {The study analyses the phenomenon of exposed bedrock within Hellenistic and Roman urban landscapes on the example of Miletus and Priene. A recurrent phenomenon addressed by this article is the unexpected, yet clearly purposeful, lack of design amid built-up architectural spaces, providing a new clue, as I will suggest, to the understanding of the aesthetics of monumental architecture. This leads to a more general discussion of Hellenistic and Imperial urban landscapes, in particular the (changing) relationship between monumental architecture and urban infrastructure, and the pehnomenon of empty spaces within towns. The fundamental claim of this article served as contrast for enhancing the impression of lavish urban architecture, and therefore fulfilled, an essential function in framing the latter.},
