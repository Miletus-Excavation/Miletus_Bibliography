@phdthesis{yasar_MiletMuzesinde_2010,
 abstract = {The subject of our study, the Milet Museum have been acquired through several years of buying a group belonging to the Hellenistic and Roman Periods terra cotta unguentaria. {\. I}n totally, We?re investigated 145 vessel. This vessels, used mainly for to keep perfum products, oils, creams and some cosmetics products. They are found very different areas. For example, in public areas, in religious sanctuary; are found mostly in the necropolis area. {\. I}n the our study these vessels ,we think that finds from the burial.Where they produced firstly? Actually, We dont know, but we know that, they used from Hellenistic period to Early Byzantine period. Our study is generally including they main typology and sub-typology, relatively chronology, functions and certain characteristics peculiarity of this vessels. Hellenistic and Roman Periods as shape which have very different forms and sub?forms. Unguentaria, was producud in two basic shapes:First shape: Fusiform or spindle, becomes popular from 4th century to first century BC.Second shape: Bulbous, becomes popular during the first century BC.{\. I}n the our study that earliest unguentarium belongs to the 2nd half of the 4th century BC, and latest example belongs to 2nd century AD.We believed that, Unguentaria of Milet Museum is very important, for region of this area in Hellenistic and Roman Period. Various forms with South Ionia and North Caria will enrich the repertoire unguentarium.Keywords: Hellenistic Period, fusiform, bulbous, conical base, Milet, Mylasa, Roman Period, Stratonikeia, Unguentaria,},
 address = {Ayd{\i}n},
 author = {Ya{\c s}ar, Ali},
 keywords = {03 Funde aus Milet, 03-05 Funde: Keramik, 03-05-06 Keramik: hellenistische Zeit, 03-05-07 Keramik: Kaiserzeit, Hellenistisch, Keramik, R{\"o}misch, Unguentarien},
 school = {Adnan Menderes {\"U}niversitesi},
 title = {{M}ilet {M}{\"u}zesi'nde bulunan {H}ellenistik ve {R}oma {D}{\"o}nemi unguentariumlar},
 type = {Masterarbeit},
 year = {2010}
}

@phdthesis{aslan_Miletosnekropolisinde_2019,
 abstract = {The present work entitled " Ceramic Finds From The Tomb of DT2012.2M from Necropol{\i}s Of M{\i}letos" consists of four parts. At first, the history of Miletos is chronologically narrated. Furthermore, the thesis tries to make the Milet = Millawanda parable understandable by describing the origins called Mycenaean and Ahhiyawa based on written sources of the contemporary Egyptians and Hittites. After the Mycenaean burial rites and tombs have been introduced, the grave, called DT2012, which has come to light in the Necropolis of Miletus, is determined. Subsequently, the ceramic finds, which represent the largest part of the grave goods of DT2012, are compared with the ceramic finds from the with similar examples in continental greece and finally evaluated. The closure part of the work is the catalog.},
 address = {Denizli},
 author = {Aslan, Arife},
 keywords = {03 Funde aus Milet, 03-05 Funde: Keramik, 03-05-02 Keramik: Bronzezeit, 09 Gr{\"a}ber und Bestattungen, Bronzezeit, Keramik},
 school = {Pamukkale {\"U}niversitesi},
 title = {{M}iletos nekropolisi\textquotesingle nde dt2012.2m mezar{\i} seramik buluntular{\i}},
