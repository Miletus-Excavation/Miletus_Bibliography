 keywords = {09 Gr{\"a}ber und Bestattungen, De{\u g}irmentepe, Grab, Kalabaktepe, Kazartepe, Nekropole, Zeytintepe},
 school = {Ruhr-Universit{\"a}t Bochum},
 title = {{D}ie {N}ekropolen von {M}ilet. {D}ie {G}rabungen von 1901 bis 1909 und die {A}rbeiten der {J}ahre 1993 bis 1996},
