@thesis{yasar_MiletMuzesinde_2010,
  type = {Masterarbeit},
  title = {Milet Müzesi'nde bulunan Hellenistik ve Roma Dönemi unguentariumlar},
  author = {Yaşar, Ali},
  date = {2010},
  institution = {Adnan Menderes Üniversitesi},
  location = {Aydın},
  abstract = {The subject of our study, the Milet Museum have been acquired through several years of buying a group belonging to the Hellenistic and Roman Periods terra cotta unguentaria. İn totally, We?re investigated 145 vessel. This vessels, used mainly for to keep perfum products, oils, creams and some cosmetics products. They are found very different areas. For example, in public areas, in religious sanctuary; are found mostly in the necropolis area. İn the our study these vessels ,we think that finds from the burial.Where they produced firstly? Actually, We dont know, but we know that, they used from Hellenistic period to Early Byzantine period. Our study is generally including they main typology and sub-typology, relatively chronology, functions and certain characteristics peculiarity of this vessels. Hellenistic and Roman Periods as shape which have very different forms and sub?forms. Unguentaria, was producud in two basic shapes:First shape: Fusiform or spindle, becomes popular from 4th century to first century BC.Second shape: Bulbous, becomes popular during the first century BC.İn the our study that earliest unguentarium belongs to the 2nd half of the 4th century BC, and latest example belongs to 2nd century AD.We believed that, Unguentaria of Milet Museum is very important, for region of this area in Hellenistic and Roman Period. Various forms with South Ionia and North Caria will enrich the repertoire unguentarium.Keywords: Hellenistic Period, fusiform, bulbous, conical base, Milet, Mylasa, Roman Period, Stratonikeia, Unguentaria,},
  keywords = {03 Funde aus Milet,03-05 Funde: Keramik,03-05-06 Keramik: hellenistische Zeit,03-05-07 Keramik: Kaiserzeit,Hellenistisch,Keramik,Römisch,Unguentarien},
