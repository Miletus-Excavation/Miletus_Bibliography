@incollection{triantafyllidis_ArchaeologicalResearchesMilesian_2015,
 abstract = {Agathonisi, in classical sources ancient Tragaia, is the northernmost island of the Dodecanesian island group in the  Aegean. Its geographical position marks the marine bounder between the Dodecanese and the neighboring western Anatolian coastline, specifically the estuary of the Meandrus river and the area of ancient Miletus. The current archaeological researches in the fort at the site called “Kastraki” reveal the siginificant fortified harbour of ancient Tragaia which flourished especially during the Hellenistic period (3rd--1st cent. {BC}.). From the study of the vast number of small finds we are able to reconstitute aspects of the residents’ daily activities (fishing, beekeeping, weaving and pottery production), their religious beliefs and practices. Their  productive activities indicate a thriving economy, trade and political contacts with the Ionic metropolis, Miletus, and the other  Aegean islands, but also with major cities of Asia Minor during the Hellenistic and Roman periods.},
 author = {Triantafyllidis, Pavlos},
 booktitle = {Recent Studies on the Archaeology of Anatolia},
 date = {2015},
 editor = {Laflı, Ergün and Patacı, Sami and Cankardeş-Şenol, Gonca},
