@thesis{cheba_BiogenInduzierteKalkkrusten_1999,
 abstract = {Die Marmorobjekte in der Ausgrabungsstätte von Milet weisen an ihrer Oberfläche stark verfremdende Krusten auf. Als Ursache für die Krustenbildung wird die zyklische Überflutung weiter Teile des Grabungsgeländes angesehen. In dieser Diplomarbeit wird die Mitwirkung von Mikroorganismen am Entstehungsprozess dieser Krusten untersucht. Neben der Charakterisierung unterschiedlicher Ausprägungsformen der Krusten wird durch verschiedene Untersuchungen ihre Zusammensetzung und ihre Auswirkung auf die Marmorsubstanz bestimmt. Anhand der auftretenden Schadensphänomene an den Marmorobjekten wird deutlich, dass die Krusten ein Schadenspotential darstellen. Die Entwicklung von geeigneten Maßnahmen zu ihrer Abnahme bildet einen weiteren Schwerpunkt dieser Arbeit.},
 author = {Cheba, Sabina},
 date = {1999},
 institution = {{FH} Köln - Institut für Konservierungs- und Restaurierungswissenschaften},
 keyword = {13 Umlandforschung, 15 Ruinen und Geländepflege, Restaurierung},
 location = {Köln},
 title = {Biogen induzierte Kalkkrusten auf Marmorobjekten in der Ausgrabungsstätte Milet/Türkei. Untersuchung zum Krustenaufbau und Entwicklung von Konzepten zu ihrer Abnahme},
 type = {Diplomarbeit}
}

