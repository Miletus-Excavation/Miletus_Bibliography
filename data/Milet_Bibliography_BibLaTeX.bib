 issn = {2196-6826, 1618-162X},
 journaltitle = {i-com},
 keyword = {07 Architektur, 07-03 Architektur: Theater, 16 Methodik, Vermessungsarbeiten},
 langid = {english},
 number = {1},
 pages = {7--18},
 title = {Immersive Inscribed Spaces -- Bringing Virtuality to Written Artefacts for Humanities},
 url = {https://www.degruyter.com/document/doi/10.1515/icom-2022-0012/html},
 urldate = {2023-01-25},
 volume = {21}
}

