@article{schaus_PainterTarantoFish_2015,
 abstract = {The Painter of the Taranto Fish is the earliest distinct personality in Laconian vase painting. Previously he has had only thirteen or fourteen vases attributed to his hand, but with the discovery of Laconian pottery at Miletus by this Painter, many other known vases, including some especially distinguished ones, can be attributed to him either directly or with strong probability, including the name vases of the Grammichele Painter and the Painter of Lakaina 1016. This changes our knowledge of his work significantly, placing him among the major Laconian painters, and an important influence in the development of the Laconian style ca. 595 -- 575 B.C. His work is found often in export markets, especially Italy-Sicily and the eastern Aegean, and from this it is clear that he helped to establish a solid customer base for Laconian pottery overseas.},
 author = {Schaus, Gerald P.},
 date = {2015},
 journaltitle = {Jahrbuch des Deutschen Archäologischen Instituts},
 keyword = {03 Funde aus Milet, 03-05 Funde: Keramik, 03-05-04 Keramik: archaische Zeit, Keramik - Gefäßmalerei, Schwarzfigurig},
 pages = {1--73},
