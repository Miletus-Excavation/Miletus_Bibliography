@incollection{bruggerhoff_CarbonateCrustsMarble_1999,
 abstract = {Marble objects at the excavation site in Milet, Turkey are often covered with grey-brownish to violet carbonate crusts which dramatically change the appearance of the objects. This paper describes the situation in Milet and presents the results of crust analysis by microscopic, chemical and petrophysical techniques. Based on the results yielded we believe that exogenous carbonate crusts are formed due to microbiological activity. This is influenced by periodic flooding of parts of the site. Hypersaline conditions and varying water levels have created a special environment which supports complex microorganism growth. Initial results of biological analysis point to the fact that cyanobacteria are mainly involved into the process of cyclic crust growth. Even though crusts have been detected mainly on surfaces there is a need to remove these disturbing and sometimes endangering layers. Tests with different cleaning techniques - laser, mechanical and chemical procedures - have been performed. Mechanical treatment has proved to be a suitable technique for cleaning as well as for the preservation of the original marble surface.},
 author = {Brüggerhoff, Stefan and Cheba, Sabina and Leisen, Hans and Warscheid, Thomas},
 booktitle = {{ICOM} Committee for Conservation, 12th Triennial Meeting Lyon},
 date = {1999},
 keyword = {15 Ruinen und Geländepflege, Restaurierung},
 location = {London},
 pages = {731--736},
 title = {Carbonate Crusts on Marble Fragments at the Excavation Site in Milet, Turkey: Examination of Crust Formation and First Results of a Cleaning Concept}
}

