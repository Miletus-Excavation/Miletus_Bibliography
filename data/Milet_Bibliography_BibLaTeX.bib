 date = {2023},
 journaltitle = {Japan Studies in Classical Antiquity},
 keyword = {08 Kulte und Kulteinrichtungen, 08-01 Kulte: Varia, 10 politische Geschichte, 10-03 Geschichte: klassische Zeit, 10-04 Geschichte: hellenistische Zeit, Didyma},
