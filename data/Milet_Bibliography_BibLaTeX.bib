@incollection{taschner_MiletKonservierungTourismus_2014,
  title = {Milet – Konservierung, Tourismus und Umweltbedingungen. Milet – Koruma, Turizm ve Çevre Koşulları},
  booktitle = {Heritage in context : Konservierung und Site Management im natürlichen, urbanen und sozialen Raum. Doğal, kentsel ve sosyal çerçevede koruma ve alan yönetimi},
  author = {Taschner, Moritz},
  editor = {Bachmann, Martin and Maner, Çiğdem and Tezer, Seçil and Göçmen, Duygu},
  date = {2014},
  series = {Miras},
  number = {2},
  pages = {253--268},
  publisher = {Ege Yayınları},
  location = {Istanbul},
  abstract = {Die Ausgrabungen in Milet haben eine lange Tradition, die bis ins 19. Jahrhundert zurückreicht. Von Anfang an haben die Ausgrabungen in Milet dazu beigetragen, grundlegende wissenschaftliche und methodische Fragen in der Archäologie aufzuwerfen; in Bezug auf Fragen der Erhaltung des kulturellen Erbes steht Milet auch heute noch vor großen Herausforderungen. Neben der großen Ausdehnung der Ausgrabungsstätte selbst stellen regelmäßige Überschwemmungen eine große Bedrohung für den Schutz der Ruinen und die touristische Erschließung der Stätte dar. Um weitere Schäden zu verhindern, hat das Deutsche Archäologische Institut im Herbst 2012 eine Kampagne gestartet. In einem ersten Schritt wurden Symptome, Ursachen und Auswirkungen der Überschwemmungen analysiert. Auch wenn die Ergebnisse noch keine endgültigen Antworten liefern können, ermöglichen sie eine systematische Analyse, die bei der Entwicklung von Präventionsmaßnahmen für archäologische Stätten mit ähnlichen Problemen hilfreich sein kann. Milet wird jedes Jahr im Spätherbst überflutet, wobei einige Ruinen bis zum Sommer des folgenden Jahres überschwemmt sind. In diesen Monaten ist das Zentrum der Stätte nicht zugänglich, und kleinere Gebäudeteile sind nicht sichtbar. Ein weiteres Problem stellen Mikroorganismen dar, die sich in stehendem Wasser bei steigenden Temperaturen ansiedeln und auf Marmor und anderen Steinen feste Krusten bilden, die diese Oberflächen dauerhaft überziehen. Neben diesen potenziellen Schadensfaktoren werden in diesem Beitrag verschiedene andere negative Auswirkungen der Überschwemmungen erörtert. Insbesondere werden Lösungen vorschlagen, die sich aus den veränderten Rahmenbedingungen vor Ort ergeben.},
  keywords = {11 Geographie und Geophysik,15 Ruinen und Geländepflege; Restaurierung,Überschwemmung},
  timestamp = {2022-12-15T17:33:44Z}
}

