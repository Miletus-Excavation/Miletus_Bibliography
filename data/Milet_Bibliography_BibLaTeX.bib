@incollection{schaus_LaconiaEastGreece_2018,
  title = {Laconia and East Greece. Cultural Exchange in the Archaic Period},
  booktitle = {Archaic and Classical Western Anatolia : new Perspectives in Ceramic Studies: In memoriam Prof. Crawford H. Greenewalt Jr: proceedings of the Second KERAMOS International Conference at Ege University, Izmir, 3-5 June, 2015},
  author = {Schaus, Gerald P.},
  editor = {Gül Gürtekin Demir, Rafet and Cevizoğlu, Hüseyin and Polat, Yasemin and Polat, Gürcan},
  date = {2018},
  series = {Colloquia Antiqua},
  number = {19},
  pages = {285--302},
  publisher = {Peeters},
  location = {Leuven},
  abstract = {The notion of a ‘special relationship’ between Sparta and Samos in the Archaic period has been widely accepted since it was first proposed by E.A. Lane in 1934, and carefully discussed by P. Cartledge in 1982, based especially on Laconian pottery finds at Samos, and information in Herodotus. It has become clear in more recent years, however, that Laconians were in regular contact with East Greeks generally, as well as with Lydians, in the Archaic period, for political, economic and cultural reasons. Recent Laconian pottery finds at Miletus suggest that Samos was not an exclusive destination in East Greece for significant quantities of this ware.},
  keywords = {03 Funde aus Milet,03-05 Funde: Keramik,03-05-04 Keramik: archaische Zeit,Archaisch},
  timestamp = {2022-12-15T17:33:23Z}
}

@article{schaus_PainterTarantoFish_2015,
  title = {The Painter of the Taranto Fish. An Early Laconian Black-Figure Vase Painter},
  author = {Schaus, Gerald P.},
  date = {2015},
  journaltitle = {Jahrbuch des Deutschen Archäologischen Instituts},
  shortjournal = {JdI},
  volume = {130},
  pages = {1--73},
  abstract = {The Painter of the Taranto Fish is the earliest distinct personality in Laconian vase painting. Previously he has had only thirteen or fourteen vases attributed to his hand, but with the discovery of Laconian pottery at Miletus by this Painter, many other known vases, including some especially distinguished ones, can be attributed to him either directly or with strong probability, including the name vases of the Grammichele Painter and the Painter of Lakaina 1016. This changes our knowledge of his work significantly, placing him among the major Laconian painters, and an important influence in the development of the Laconian style ca. 595 – 575 B.C. His work is found often in export markets, especially Italy-Sicily and the eastern Aegean, and from this it is clear that he helped to establish a solid customer base for Laconian pottery overseas.},
  keywords = {03 Funde aus Milet,03-05 Funde: Keramik,03-05-04 Keramik: archaische Zeit,Keramik - Gefäßmalerei,Schwarzfigurig},
  timestamp = {2022-12-15T17:33:38Z}
