@incollection{donder_MetallfundeVomKalabaktepe_2016,
 abstract = {Es wird mit Hilfe der metallischen Funde aus Milet versucht, Antwort auf die eingangs gestellte Frage zu finden. In kurzer Form wird die Topographie Milets in archaischer Zeit dargestellt. In diesem bedeutenden Abschnitt der Ortsgeschichte hatte das milesische Stadtgebiet seine größte Ausdehnung erreicht. Deutlich illustriert wird dies durch die Errichtung der Stadtmauern in dieser Zeit. Im Süden Milets umschließen sie einen Kalabaktepe genannten Hügel. Grabungen in den 80er und 90er Jahren des vergangenen Jahrhunderts haben für diesen Bereich erwiesen, dass die Besiedlung am Fuß und auf der östlichen Terrasse des Kalabaktepe einen Teil der archaischen Stadt bildete. Der Charakter dieser Besiedlung spricht für Wohn- und Handwerkerviertel. In einem kurzen Überblick stelle ich die Baugeschichte dieser Besiedlung dar. Anschließend wird eine Aufzählung der verschiedenen Fundgattungen gegeben und es wird versucht, einen Beitrag zur Problemstellung dieser Tagung zu leisten.},
 author = {Donder, Helga},
 booktitle = {Materielle Kultur und Identität im Spannungsfeld zwischen mediterraner Welt und Mitteleuropa: Akten der internationalen Tagung am Römisch-Germanischen Zentralmuseum Mainz, 22.-24. Oktober 2014: Abschlusstagung des {DFG}-Projekts "Metallfunde als Zeugnis für die Interaktion zwischen Griechen und Indigenen auf Sizilien zwischen dem 8. und 5. Jahrhundert v. Chr.},
 date = {2016},
 editor = {Baitinger, Holger},
 keyword = {03 Funde aus Milet, 03-07 Funde: Metalle, Archaisch, Baugeschichte, Kalabaktepe, Metall, Topographie},
 location = {Mainz},
 number = {27},
 pages = {175--184},
 publisher = {Verlag des Römisch-Germanischen Zentralmuseums},
 series = {{RGZM} Tagungen},
 title = {Die Metallfunde vom Kalabaktepe in Milet: Siedlungsniederschlag oder thesauriertes Altmetall?}
}

@article{dietrich_ExposedBedrockMiletus_2016,
 abstract = {The study analyses the phenomenon of exposed bedrock within Hellenistic and Roman urban landscapes on the example of Miletus and Priene. A recurrent phenomenon addressed by this article is the unexpected, yet clearly purposeful, lack of design amid built-up architectural spaces, providing a new clue, as I will suggest, to the understanding of the aesthetics of monumental architecture. This leads to a more general discussion of Hellenistic and Imperial urban landscapes, in particular the (changing) relationship between monumental architecture and urban infrastructure, and the pehnomenon of empty spaces within towns. The fundamental claim of this article served as contrast for enhancing the impression of lavish urban architecture, and therefore fulfilled, an essential function in framing the latter.},
 author = {Dietrich, Nikolaus},
 date = {2016},
