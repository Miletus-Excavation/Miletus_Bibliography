@book{henke_ZyprischenTerrakottenAus_2017,
 abstract = {In vielen südostgriechischen Heiligtümern stellen archaische zyprische Terrakotten eine reiche Fundgruppe dar. Nach den jüngeren Grabungen lässt sich auch Milet mit einer stattlichen Fundmenge diesen Orten anschließen. In dem Band werden die neuen milesischen Funde wissenschaftlich vorgelegt und ihre Stellung innerhalb des südostägäischen Gesamtkomplexes zyprischer Koroplastik diskutiert. Dabei steht erneut dessen stilchronologische und produktionstechnische Einordnung im Vordergrund. Aufbauend auf den regionalstilistischen Studien Sabine Fourriers auf Zypern werden die engeren werkstattspezifischen Verbindungen der südostägäischen Funde untersucht und es wird nach deren Herkunft auf Zypern gefragt. Hierbei gelingen u. a. ergänzende Einblicke in die verschiedenen Herstellungsprozesse der Terrakotten sowie der Nachweis von unterschiedlichen Serien an hohl und massiv mit Matrizen hergestellten Figuren. In Einzelfällen scheinen sogar individuelle Modellierungen konkreter Koroplasten oder eng zusammenarbeitender Handwerkergruppen erkennbar, die es erlauben, ein umfangreicheres Œuvre über verschiedene Figurenformate und Produktionsweisen hinweg nachzuzeichnen. Damit verengt sich das Bild für den Großteil des südostägäischen Fundmaterials, für das schon früh eine auffällige stilistische Homogenität erkannt wurde, auf sehr wenige in einem engeren zeitlichen wie räumlichen Rahmen arbeitende Werkstätten aus dem unmittelbaren topographischen Umfeld von Salamis auf Zypern.
Die Ergebnisse erweitern und schärfen die bisherigen stilistischen und produktionstechnischen Untersuchungen, können diese aber auch in Einzelfällen korrigieren. Auf Grundlage der neuen Grabungsergebnisse in Milet und der Überprüfung der samischen Kontexte zyprischer Koroplastik wird eine neue Datierung für den Import der Terrakotten in die Südostägäis vorgeschlagen, der konträr zu den bisherigen Ansätzen innerhalb weniger Jahrzehnte bis ca. 630 v. Chr. stattgefunden hat. Daneben leiten die neuen Funde an wahrscheinlich in Milet hergestellten zyprisierenden Terrakotten zur Diskussion von technischen Anregungen auf das südostägäische Koroplastenhandwerk über, die die heimischen Handwerker durch Autopsie der umfangreichen zyprischen Importe erhalten haben könnten. Schließlich werden die Terrakotten als Zeugnisse der regen zwischenkulturellen Austauschprozesse im früheisenzeitlichen Mittelmeerraum hervorgehoben und nach der Identität der Stiftenden gefragt. Dabei könnten spezifische Verteilungsmuster der Figuren darauf hindeuten, dass sie nach konkreten technischen wie inhaltlichen Aspekten von der Lokalbevölkerung ausgewählt wurden.},
 author = {Henke, Jan-Marc},
 date = {2017},
 keyword = {03 Funde aus Milet, 03-06 Funde: Terrakotta, 08 Kulte und Kulteinrichtungen, 08-06 Kulte: Aphrodite von Oikous, Aphroditeheiligtum, Archaisch, Terrakotten, Werkstatt, Zeytintepe, Zypern},
 location = {Berlin},
 number = {7},
 publisher = {Deutsches Archäologisches Institut},
 series = {Milesische Forschungen},
 title = {Die zyprischen Terrakotten aus Milet: Neue Überlegungen zur Einordnung der archaischen zyprischen Terrakotten aus ostägäischen Fundkontexten und ihrer werkstattspezifischen Zuweisung},
 url = {https://www.doi.org/10.34780/1h67-m6dd}
}

